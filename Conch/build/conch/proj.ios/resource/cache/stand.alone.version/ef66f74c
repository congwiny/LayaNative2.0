(x:number,y:number,direction:number):void{\r\n        var data:object={};\r\n        data[\"game_uid\"]=ModelUser.ins.game_uid;\r\n        data[\"nickname\"]=ModelUser.ins.nickname;\r\n        data[\"role_id\"]=ModelUser.ins.role_id;\r\n        data[\"x\"]=x;\r\n        data[\"y\"]=y;\r\n        data[\"direction\"]=direction;\r\n        GameProtocol.ins.sendMessage(ProtocolAction.updateLocation,data);\r\n    }\r\n    \r\n    public respond_updateLocation(data:object):void{\r\n        //广播给自己的就不要处理了\r\n        var game_uid:string=data[\"game_uid\"];\r\n        var playerVo:PlayerVo=ModelMap.ins.getPlayer(game_uid);\r\n        if(playerVo){\r\n            playerVo.x=data[\"x\"];\r\n            playerVo.y=data[\"y\"];\r\n            playerVo.direction=data[\"playerVo\"];\r\n        }\r\n        if(game_uid==ModelUser.ins.game_uid){\r\n            return;\r\n        }\r\n        if (data[\"time\"] == null)\r\n        {\r\n            data[\"time\"] = Browser.now();\r\n        }\r\n        ModelMap.ins.event(MapEvent.UPDATE_LOCATION,data);\r\n    }\r\n    public send_buildEntity(entity_id:number,x:number,y:number):void{\r\n        var data:object={};\r\n        data[\"game_uid\"]=ModelUser.ins.game_uid;\r\n        data[\"entity_id\"]=entity_id;\r\n        data[\"x\"]=x;\r\n        data[\"y\"]=y;\r\n        GameProtocol.ins.sendMessage(ProtocolAction.buildEntity,data);\r\n    }\r\n    public respond_addEntity(data:object):void{\r\n        var entityVo:EntityVo=ProtocolUtils.createEntityVo(data);\r\n        ModelMap.ins.addEntity(entityVo);\r\n        ModelMap.ins.event(MapEvent.ADD_ENTITY,entityVo);\r\n    }\r\n    public send_removeEntity(guid:number):void{\r\n        var data:object={};\r\n        data[\"game_uid\"]=ModelUser.ins.game_uid;\r\n        data[\"guid\"]=guid;\r\n        GameProtocol.ins.sendMessage(ProtocolAction.removeEntity,data);\r\n    }\r\n    public respond_removeEntity(data:object):void{\r\n        var guid:number=data[\"guid\"];\r\n        ModelMap.i
able';\r\nimport LangKey from '../constant/LangKey';\r\nexport default class LangUtils {\r\n\t//单纯获取标签 appendSing=是否带冒号\r\n\tpublic static getLabel(key:string,appendSign:boolean=true):string{\r\n\t\treturn LangUtils.getLang(key)+(appendSign?\":\":\"\");\r\n\t}\r\n\t//获取语言 {0}测试{1}测试\r\n\tpublic static getLang(key: string, params: Array<any> = null): string {\r\n\t\tvar check: Function = function (str, index, value): string {\r\n\t\t\treturn str;\r\n\t\t}\r\n\t\tvar lang: string = DictionaryTable.ins.getConfig(key);\r\n\t\tif(!lang){\r\n\t\t\tlang=key;\r\n\t\t}\r\n\t\treturn StringUtils.format(lang, params, check);\r\n\t}\r\n\t//根据列表键返回列表语言\r\n\tpublic static getListLang(keys: Array<string>): Array<string> {\r\n\t\tvar list: Array<string> = [];\r\n\t\tfor (var i: number = 0; i < keys.length; i++) {\r\n\t\t\tlist.push(LangUtils.getLang(keys[i]));\r\n\t\t}\r\n\t\treturn list;\r\n\t}\r\n\t//获取TabBar的标签文本数组\r\n\tpublic static getTabLabels(keys: Array<string>): string {\r\n\t\treturn LangUtils.getListLang(keys).join(\",\");\r\n\t}\r\n}\r\n","import PanelManager from \"../../managers/PanelManager\";\r\nimport PanelBase from './PanelBase';\r\nimport { editor} from '../../editor/layaMaxUI';\r\nimport PathManager from '../../managers/PathManager';\r\nimport LangUtils from '../../utils/LangUtils';\r\nimport Image = Laya.Image;\r\nimport Event = Laya.Event;\r\nimport UIComponent = Laya.UIComponent;\r\nimport Tween = Laya.Tween;\r\nimport Ease = Laya.Ease;\r\nimport Sprite = Laya.Sprite;\r\nimport PopupPanelTitleUI=editor.share.PopupPanelTitleUI;\r\nimport Point=Laya.Point;\r\nexport default class PopupPanel extends PanelBase {\r\n\tprotected tweenFlag: boolean;\r\n\tprotected clickMaskClose: boolean;\r\n\t//ui蒙版\r\n\tprotected uiMask: Image;\r\n\tprotected viewIsFull:boolean;\r\n\tconstructor() {\r\n\t\tsuper();\r\n\t\tthis.viewIsFull=false;\r\n\t\tthis.tweenFlag = true;\r\n\t\tthis.clickMaskClose = true;\r\n\t\tthis.createMask();\r\n\t}\r\n\tprivate createMask(): void {\r\n\t\
ar hideComplete: Function = function (): void {\r\n\t\t\tPlayerMessage.floatNext();\r\n\t\t}\r\n\t\tTween.to(PlayerMessage.messageUI, { alpha: 1, y: PlayerMessage.messageUI.y - 50 }, 200, Ease.sineOut, Handler.create(PlayerMessage, showComplete));\r\n\t}\r\n\t//展示滚动消息\r\n\tpublic static showMessage(playerVo:PlayerVo,msg: string, liveTime: number = 1500): void {\r\n\t\tif (GameUtils.stringIsEmpty(msg)) {\r\n\t\t\treturn;\r\n\t\t}\r\n\t\tvar playerMsgVo: PlayerMsgVo = new PlayerMsgVo();\r\n\t\tplayerMsgVo.playerVo=playerVo;\r\n\t\tplayerMsgVo.msg = msg;\r\n\t\tplayerMsgVo.liveTime = liveTime;\r\n\t\tplayerMsgVo.time = Laya.Browser.now();\r\n\t\tPlayerMessage.list.push(playerMsgVo);\r\n\t\tif (!PlayerMessage.playing) {\r\n\t\t\tPlayerMessage.floatNext();\r\n\t\t}\r\n\t}\r\n}\r\n","import MapUnit from \"./MapUnit\";\r\nimport MapScene from \"./MapScene\";\r\nimport AniEvent from \"../animation/AniEvent\";\r\nimport GameAni from \"../animation/GameAni\";\r\nimport { AniAction } from \"../animation/AniAction\";\r\nimport AnimationKey from \"../constant/AnimationKey\";\r\n\r\nexport default class EffectUnit extends MapUnit{\r\n    public static curuid:number = 0;\r\n    private avatar:GameAni;\r\n    public uid:number;\r\n\r\n    constructor(mapScene:MapScene)\r\n    {\r\n        super(mapScene);\r\n        EffectUnit.curuid ++;\r\n        this.uid = EffectUnit.curuid;\r\n        this.avatar=new GameAni();\r\n        this.avatar.on(AniEvent.PLAY_END,this,this.onAniPlayEnd);\r\n        this.addChild(this.avatar);\r\n    }\r\n\r\n    public init(resId:string):void\r\n    {\r\n        this.avatar.init(resId,false);\r\n        if (resId == AnimationKey.aixin)\r\n        {\r\n            this.avatar.interval = Math.floor(Config.animationInterval*0.5);\r\n        }\r\n        else\r\n        {\r\n            this.avatar.interval = Config.animationInterval;\r\n        }\r\n        this.avatar.play(AniAction.Idle,false,true);\r\n    }\r\n\r\n    private onAniPlayEnd():void\r\n    
umber=Math.floor(time / TimeUtils.oneDayMs);\r\n\t\tif(format.indexOf('d')>=0){\r\n\t\t\ttime=time % TimeUtils.oneDayMs;\r\n\t\t}\r\n\t\tvar h_num:number=Math.floor(time / TimeUtils.oneHourMs);\r\n\t\tif(format.indexOf('H')>=0){\r\n\t\t\ttime=time % TimeUtils.oneHourMs;\r\n\t\t}\r\n\t\tvar m_num:number=Math.floor(time / TimeUtils.oneMinuteMs);\r\n\t\tif(format.indexOf('m')>=0){\r\n\t\t\ttime=time % TimeUtils.oneMinuteMs;\r\n\t\t}\r\n\t\tvar s_num:number=Math.floor(time / TimeUtils.oneSecondMs);\r\n\t\tvar params:Array<any>=[\r\n\t\t\t[\"d\",d_num,TimeUtils.appendTimeExtra(\"d\",mode)],\r\n\t\t\t[\"H\",h_num,TimeUtils.appendTimeExtra(\"H\",mode)],\r\n\t\t\t[\"m\",m_num,TimeUtils.appendTimeExtra(\"m\",mode)],\r\n\t\t\t[\"s\",s_num,TimeUtils.appendTimeExtra(\"s\",mode)],\r\n\t\t]\r\n\t\treturn TimeUtils.formatByParams(format,params,mode);\r\n\t}\r\n\tprivate static appendTimeExtra(key:string,mode:TimeFormatMode):string{\r\n\t\tvar extra:string=\"\";\r\n\t\tif(mode==TimeFormatMode.CN){\r\n\t\t\textra=LangUtils.getLang(\"time_\"+key);\r\n\t\t}else{\r\n\t\t\textra=\"\";\r\n\t\t}\r\n\t\treturn extra;\r\n\t}\r\n\tprivate static appendDateExtra(key:string,mode:TimeFormatMode):string{\r\n\t\tvar extra:string=\"\";\r\n\t\tif(mode==TimeFormatMode.CN){\r\n\t\t\textra=LangUtils.getLang(\"date_\"+key);\r\n\t\t}else{\r\n\t\t\textra=\"\";\r\n\t\t}\r\n\t\treturn extra;\r\n\t}\r\n\t//格式化剩余时间\r\n\tpublic static formatRemainTime(time:number,mode:TimeFormatMode=TimeFormatMode.CN):string{\r\n\t\tif(mode==TimeFormatMode.CN){\r\n\t\t\tvar format:string=\"\";\r\n\t\t\tif(time>TimeUtils.oneDayMs){\r\n\t\t\t\tformat=TimeFormat.time_dhms;\r\n\t\t\t}else if(time>TimeUtils.oneHourMs){\r\n\t\t\t\tformat=TimeFormat.time_hms;\r\n\t\t\t}else if(time>TimeUtils.oneMinuteMs){\r\n\t\t\t\tformat=TimeFormat.time_ms;\r\n\t\t\t}else{\r\n\t\t\t\tformat=TimeFormat.time_s;\r\n\t\t\t}\r\n\t\t\treturn TimeUtils.timeFormat(time,format,mode);\r\n\t\t}else{\r\n\t\t\treturn TimeUtils.timeFormat(time,TimeFormat.time_hms,mode);\r\n\t\t}\r\n\t}\r\n\t//根据参数格式化\r\n\tpublic static formatByParams(format:string,params:Array<any>,mode:TimeFormatMode):string{\r\n\t\tfor(var i:number=0;i<params.length;i++){\r\n\t\t\tvar key:string=params[i][0];\r\n\t\t\tvar newStr:string=params[i][1];\r\n\t\t\tvar appendStr:string=params[i][2];\r\n\t\t\tvar result:Array<any>=new RegExp(\"(\"+key+\"+\"+\")\").exec(format);\r\n\t\t\tif (result){\r\n\t\t\t\tvar matchStr:string=result[1];\r\n\t\t\t\tif (matchStr.length > 1){\r\n\t\t\t\t\tnewStr=StringUtils.fill(newStr,matchStr.length,'0',false);\r\n\t\t\t\t}\r\n\t\t\t\tformat=format.replace(matchStr,newStr+appendStr);\r\n\t\t\t};\r\n\t\t};\r\n\t\t//中文去掉冒号\r\n\t\tif(mode==TimeFormatMode.CN){\r\n\t\t\tformat=format.replace(/:/g,\"\");\r\n\t\t\tformat=format.replace(/-/g,\"\");\r\n\t\t}\r\n\t\treturn format;\r\n\t}\r\n\t//获取下一个整点时间戳\r\n\tpublic static getNextHourMs(time:number):number{\r\n\t\treturn (Math.floor(time / TimeUtils.oneHourMs)+1)*TimeUtils.oneHourMs;\r\n\t}\r\n\t//根据时间获取今天开始的时间\r\n\tpublic static getDayStartTime(time:number):number{\r\n\t\tvar date:Date=new Date(time);\r\n\t\tdate.setHours(0,0,0,0);\r\n\t\tvar crossDayTimePoint:number=date.getTime()+TimeUtils.crossDayMs;\r\n\t\tif (time < crossDayTimePoint){\r\n\t\t\treturn crossDayTimePoint-TimeUtils.oneDayMs;\r\n\t\t}\r\n\t\treturn crossDayTimePoint;\r\n\t}\r\n\t//获取恢复值 lastTime=上次恢复时间 nowTime=当前时间 duration=恢复间隔 maxValue=最大恢复值\r\n\tpublic static recoverValue(value:number,lastTime:number,nowTime:number,duration:number,maxValue:number):number{\r\n\t\treturn Math.min(maxValue,Math.floor(value+(nowTime-lastTime) / (duration*TimeUtils.oneSecondMs) ));\r\n\t}\r\n}\r\n","import SheetConfig from '../core/SheetConfig';\r\nimport ConfigManager from '../../managers/ConfigManager';\r\nimport ConfigKey from '../../constant/ConfigKey';\r\nimport StructConfig from '../core/StructConfig';\r\nimport LangUtils from '../../utils/LangUtils';\r\nimport PathManager from '../../managers/PathManager';\r\nexport class Perf
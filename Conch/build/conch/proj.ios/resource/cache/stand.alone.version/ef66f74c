  Floor = \"floor\",\r\n    Block = \"block\",\r\n    Entity = \"entity\",\r\n    Player = \"player\",\r\n    Mask = \"mask\"\r\n}\r\nexport enum BlockType {\r\n    None = 0,//空白\r\n    Block = 1,//障碍\r\n    Pass = 2,//可行走\r\n}\r\nexport default class MapScene extends GameScene implements IMap {\r\n    private _camera: MapCamera;\r\n    private _map: TiledMap;\r\n    private blockLayer: MapLayer;\r\n    private floorLayer: MapLayer;\r\n    private playerLayer: MapLayer;\r\n    private _mainPlayer: PlayerUnit;\r\n    private entityLayer: MapLayer;\r\n    private maskLayer: MapLayer;\r\n    private maskDataDic: object;\r\n    private passList: Array<Array<number>>;\r\n    private playerDic: object;\r\n    private _aStar: AStar;\r\n    public cacheInteractUid: string;\r\n    private entityDic: object;\r\n    private effectDic: object;\r\n    private MAPSCALE: number = 0.75;\r\n    private curCanInteractPlayer: PlayerUnit;\r\n    constructor() {\r\n        super();\r\n        this._camera = new MapCamera(this);\r\n        this.passList = [];\r\n        this._map = new TiledMap();\r\n        this.playerDic = {};\r\n        this._aStar = new AStar(this);\r\n        this.cacheInteractUid = \"\";\r\n        this.entityDic = {};\r\n        this.effectDic = {};\r\n    }\r\n    public get map(): TiledMap {\r\n        return this._map;\r\n    }\r\n    public get camera(): MapCamera {\r\n        return this._camera;\r\n    }\r\n    public get aStar(): AStar {\r\n        return this._aStar;\r\n    }\r\n    public get mainPlayer(): PlayerUnit {\r\n        return this._mainPlayer;\r\n    }\r\n    protected onEnter(): void {\r\n        this.initMap(ModelUser.ins.mapConfig.path);\r\n    }\r\n    private initComplete(): void {\r\n        ModelMap.ins.on(MapEvent.ADD_PLAYER, this, this.addPlayer);\r\n        GameManager.ins.on(KeyEvent.KEY_DOWN, this, this.onKeyDown);\r\n        GameManager.ins.on(KeyEvent.KEY_UP, this, this.onKeyUp);\r\n        GameManager.ins.on(KeyEvent.KEY_ACTION, this, this.onKeyAction);\r\n        GameManager.ins.on(PlayerEvent.PLAYER_MOVE, this, this.onPlayerMove);\r\n        ModelMap.ins.on(MapEvent.UPDATE_LOCATION, this, this.onUpdateLocation);\r\n        ModelMap.ins.on(MapEvent.BEHAVIOR, this, this.onBehavior);\r\n        ModelMap.ins.on(MapEvent.INTERACT, this, this.onInteract);\r\n        ModelMap.ins.on(MapEvent.REMOVE_PLAYER, this, this.onRemovePlayer);\r\n        ModelMap.ins.on(MapEvent.ADD_ENTITY, this, this.addEntity);\r\n        ModelMap.ins.on(MapEvent.REMOVE_ENTITY, this, this.onRemoveEntity);\r\n    }\r\n    protected onExit(): void {\r\n        ModelMap.ins.off(MapEvent.ADD_PLAYER, this, this.addPlayer);\r\n        GameManager.ins.off(KeyEvent.KEY_DOWN, this, this.onKeyDown);\r\n        GameManager.ins.off(KeyEvent.KEY_UP, this, this.onKeyUp);\r\n        GameManager.ins.off(KeyEvent.KEY_ACTION, this, this.onKeyAction);\r\n        GameManager.ins.off(PlayerEvent.PLAYER_MOVE, this, this.onPlayerMove);\r\n        ModelMap.ins.off(MapEvent.UPDATE_LOCATION, this, this.onUpdateLocation);\r\n        ModelMap.ins.off(MapEvent.BEHAVIOR, this, this.onBehavior);\r\n        ModelMap.ins.off(MapEvent.INTERACT, this, this.onInteract);\r\n        ModelMap.ins.off(MapEvent.REMOVE_PLAYER, this, this.onRemovePlayer);\r\n        ModelMap.ins.off(MapEvent.ADD_ENTITY, this, this.addEntity);\r\n        ModelMap.ins.off(MapEvent.REMOVE_ENTITY, this, this.onRemoveEntity);\r\n        this._map.destroy();\r\n        if (this.floorLayer) {\r\n            this.floorLayer.off(Event.CLICK, this, this.onFloorLayerClick);\r\n        }\r\n        this.floorLayer = null;\r\n        this.playerLayer = null;\r\n        this.blockLayer = null;\r\n        this.params = null;\r\n    }\r\n    private onRemoveEntity(data: object): void {\r\n        var guid: number = data[\"guid\"];\r\n        var entityUnit: EntityUnit = this.getEntity(guid);\r\n        if (!entityUnit) {\r\n            return;\r\n        }\r\n        entityUnit.destroy();\r\n        delete this.entityDic[guid];\r\n    }\r\n    private onRemovePlayer(data: object): void {\r\n        var game_uid: string = data[\"game_uid\"];\r\n        var playerUnit: PlayerUnit = this.getPlayer(game_uid);\r\n        if (!playerUnit) {\r\n            return;\r\n        }\r\n        if (playerUnit == this.curCanInteractPlayer) {\r\n            this.curCanInteractPlayer = null;\r\n        }\r\n        if (this.cacheInteractUid == game_uid) {\r\n            TipUIManager.ins.closeTipById(TipUIKey.InteractTip);\r\n            this.cacheInteractUid = \"\";\r\n        }\r\n        PlayerMessage.showMessage(playerUnit.playerVo, \"离开了房间\");\r\n        playerUnit.destroy();\r\n        delete this.playerDic[game_uid];\r\n    }\r\n    private onInteract(data: object): void {\r\n        var game_uid: string = data[\"game_uid\"];\r\n        var playerUnit: PlayerUnit = this.getPlayer(game_uid);\r\n        if (!playerUnit) {\r\n            return;\r\n        }\r\n        var target_uid: string = data[\"target_uid\"];\r\n        var interact_id: number = data[\"interact_id\"];\r\n        var params: string = data[\"params\"];\r\n        playerUnit.doInteract(interact_id, target_uid, params);\r\n    }\r\n    private onBehavior(data: object): void {\r\n        var game_uid: string = data[\"game_uid\"];\r\n        var playerUnit: PlayerUnit = this.getPlayer(game_uid);\r\n        if (!playerUnit) {\r\n            return;\r\n        }\r\n        var behavior_id: number = data[\"behavior_id\"];\r\n        var params: string = data[\"params\"];\r\n        playerUnit.doBehavior(behavior_id, params);\r\n    }\r\n    private onUpdateLocation(data: object): void {\r\n        var game_uid: string = data[\"game_uid\"];\r\n        var playerUnit: PlayerUnit = this.getPlayer(game_uid);\r\n        if (!playerUnit) {\r\n            return;\r\n        }\r\n        var x: number = data[\"x\"];\r\n        var y: number = data[\"y\"];\r\n        var direction = data[\"direction\"];\r\n        var time: number = data[\"time\"];\r\n        playerUnit.updateLocation(x, y, direction, time);\r\n    }\r\n    private onKeyDown(keyCode: number): void {\r\n        if (GameManager.ins.isTextInput == true) {\r\n            return;\r\n        }\r\n        var direction: number = RoleDir.None;\r\n        switch (keyCode) {\r\n            case Laya.Keyboard.W:\r\n            case Laya.Keyboard.UP:\r\n                direction = RoleDir.Up;\r\n                break;\r\n            case Laya.Keyboard.D:\r\n            case Laya.Keyboard.RIGHT:\r\n                direction = RoleDir.Right;\r\n                break;\r\n            case Laya.Keyboard.S:\r\n            case Laya.Keyboard.DOWN:\r\n                direction = RoleDir.Down;\r\n                break;\r\n            case Laya.Keyboard.A:\r\n            case Laya.Keyboard.LEFT:\r\n                direction = RoleDir.Left;\r\n                break;\r\n        }\r\n        //按的不是行走键\r\n        if (direction == RoleDir.None) {\r\n            return;\r\n        }\r\n        //自己不必等广播消息回来 那样会有延迟\r\n        this.mainPlayer.startMove(this.mainPlayer.virtualX, this.mainPlayer.virtualY, direction);\r\n        //关闭我自己的表演界面\r\n        TipUIManager.ins.closeTipById(TipUIKey.PerformTip);\r\n    }\r\n    private onKeyUp(keyCode: number): void {\r\n        if (GameManager.ins.isTextInput == true) {\r\n            return;\r\n        }\r\n        var direction: number = RoleDir.None;\r\n        switch (keyCode) {\r\n            case Laya.Keyboard.W:\r\n            case Laya.Keyboard.UP:\r\n                direction = RoleDir.Up;\r\n                break;\r\n            case Laya.Keyboard.D:\r\n            case Laya.Keyboard.RIGHT:\r\n                direction = RoleDir.Right;\r\n                break;\r\n            case Laya.Keyboard.S:\r\n            case Laya.Keyboard.DOWN:\r\n                direction = RoleDir.Down;\r\n                break;\r\n            case Laya.Keyboard.A:\r\n            case Laya.Keyboard.LEFT:\r\n                direction = RoleDir.Left;\r\n                break;\r\n        }\r\n        //弹起的不是行走键\r\n        if (direction == RoleDir.None) {\r\n            return;\r\n        }\r\n        //自己不必等广播消息回来 那样会有延迟\r\n        this.mainPlayer.stopMove(direction);\r\n    }\r\n    private onKeyAction(keyCode: number): void {\r\n        if (EntityManager.ins.status != EntityBuildStatus.None) {\r\n            return;\r\n        }\r\n        if (GameManager.ins.isTextInput == true) {\r\n            return;\r\n        }\r\n        switch (keyCode) {\r\n            case Laya.Keyboard.Q:\r\n                this.mainPlayer.onClick();\r\n                break;\r\n            case Laya.Keyboard.E:\r\n                if (this.curCanInteractPlayer) {\r\n                    this.curCanInteractPlayer.onClick();\r\n                }\r\n                else {\r\n                    FloatMessage.showMessage(\"附近没有可互动的目标\");\r\n                    return;\r\n                }\r\n                break;\r\n        }\r\n    }\r\n\r\n    public onPlayerMove(player: PlayerUnit): void  {\r\n        if (player == this.mainPlayer || player == this.curCanInteractPlayer) {\r\n            var nearstPlayer: PlayerUnit = this.getNearestIntractPlayer();\r\n            if (nearstPlayer != this.curCanInteractPlayer) {\r\n                if (this.curCanInteractPlayer) {\r\n                    this.curCanInteractPlayer.hideIntractTip();\r\n                }\r\n                this.curCanInteractPlayer = nearstPlayer;\r\n                if (nearstPlayer != null) {\r\n                    nearstPlayer.showIntractTip();\r\n                }\r\n            }\r\n        }\r\n        else {\r\n            var myTile: Point = this.mainPlayer.getStandTile();\r\n            var pTile: Point = player.getStandTile();\r\n            var disp: number = myTile.distance(pTile.x, pTile.y);\r\n            if (disp <= PlayerUnit.INTRACT_DISTANCE) {\r\n                if (this.curCanInteractPlayer) {\r\n                    var cTile = this.curCanInteractPlayer.getStandTile();\r\n                    var disc = myTile.distance(cTile.x, cTile.y);\r\n                    if (disp < disc) {\r\n                        this.curCanInteractPlayer.hideIntractTip();\r\n                        this.curCanInteractPlayer = player;\r\n                        player.showIntractTip();\r\n                    }\r\n                }\r\n                else {\r\n                    player.showIntractTip();\r\n                    this.curCanInteractPlayer = player;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    public getNearestIntractPlayer(): PlayerUnit {\r\n        var arr = [];\r\n        for (var key in this.playerDic) {\r\n            if (this.playerDic[key] != this.mainPlayer) {\r\n                arr.push(this.playerDic[key]);\r\n            }\r\n        }\r\n        var myTile: Point = this.mainPlayer.getStandTile();\r\n        arr.sort(function (a: PlayerUnit, b: PlayerUnit): number {\r\n            var atile: Point = a.getStandTile();\r\n            var disa: number = myTile.distance(atile.x, atile.y);\r\n            var btile: Point = b.getStandTile();\r\n            var disb: number = myTile.distance(btile.x, btile.y);\r\n            return disa - disb;\r\n        })\r\n\r\n        var target: PlayerUnit = arr[0];\r\n        if (target) {\r\n            var ttile: Point = target.getStandTile();\r\n            var dist: number = myTile.distance(ttile.x, ttile.y);\r\n            if (dist > PlayerUnit.INTRACT_DISTANCE) {\r\n                return null;\r\n            }\r\n            else {\r\n                return target;\r\n            }\r\n        }\r\n        else {\r\n            return null\r\n        }\r\n    }\r\n\r\n    public setAllEntitysHitArea(clear: boolean): void {\r\n        for (var key in this.entityDic) {\r\n            this.entityDic[key].setHitArea(clear);\r\n        }\r\n    }\r\n    public setAllPlayersHitArea(clear: boolean): void {\r\n        for (var key in this.playerDic) {\r\n            this.playerDic[key].setHitArea(clear);\r\n        }\r\n    }\r\n    public getEntity(guid: number): EntityUnit {\r\n        return this.entityDic[guid];\r\n    }\r\n    private addEntity(data: EntityVo): EntityUnit {\r\n        var entityUnit: EntityUnit = this.getEntity(data.guid);\r\n        //已经存在了\r\n        if (entityUnit) {\r\n            return;\r\n        }\r\n        entityUnit = new EntityUnit(this);\r\n        this.entityDic[data.guid] = entityUnit;\r\n        entityUnit.init(data);\r\n        if (data.entityConfig.isFloor) {\r\n            this.entityLayer.addChild(entityUnit);\r\n        } else {\r\n            this.playerLayer.addChild(entityUnit);\r\n        }\r\n        return entityUnit;\r\n    }\r\n    public getPlayer(uid: string): PlayerUnit {\r\n        return this.playerDic[uid];\r\n    }\r\n    private addPlayer(data: PlayerVo, showMsg = true): PlayerUnit {\r\n        var playerUnit: PlayerUnit = this.getPlayer(data.game_uid);\r\n        //已经存在了\r\n        if (playerUnit) {\r\n            return;\r\n        }\r\n        if (showMsg) {\r\n            PlayerMessage.showMessage(data, \"进入了房间\");\r\n        }\r\n        playerUnit = new PlayerUnit(this);\r\n        this.playerDic[data.game_uid] = playerUnit;\r\n        playerUnit.init(data);\r\n        this.playerLayer.addChild(playerUnit);\r\n        return playerUnit;\r\n    }\r\n\r\n    public playEffect(name: string, vx: number, vy: number, zorder:number): void {\r\n        var effect: EffectUnit = new EffectUnit(this);\r\n        effect.init(name);\r\n        effect.setVirtualPos(vx, vy);\r\n        effect.zOrder = zorder;\r\n        this.effectDic[effect.uid] = effect;\r\n        this.playerLayer.addChild(effect);\r\n    }\r\n    public removeEffect(effect: EffectUnit): void {\r\n        delete this.effectDic[effect.uid];\r\n        effect.destroy();\r\n    }\r\n\r\n    protected initMap(mapPath: string): void {\r\n        var viewRect: Rectangle = new Rectangle(0, 0, Laya.stage.width, Laya.stage.height);\r\n        var size: Point = new Point(256, 256);\r\n        var padding: Rectangle = new Rectangle(100, 100, 100, 100);\r\n        this._map.createMap(mapPath, viewRect, Handler.create(this, this.onLoaded), padding, size);\r\n    }\r\n    protected onLoaded(): void {\r\n        this.addChild(this._map.mapSprite());\r\n        this.floorLayer = this._map.getLayerByName(LayerName.Floor);\r\n        this.blockLayer = this._map.getLayerByName(LayerName.Block);\r\n        this.entityLayer = this._map.getLayerByName(LayerName.Entity);\r\n        this.playerLayer = this._map.getLayerByName(LayerName.Player);\r\n        this.maskLayer = this._map.getLayerByName(LayerName.Mask);\r\n        this.maskLayer.alpha = 0.5;\r\n        this.maskDataDic = this.maskLayer[\"_dataDic\"];\r\n        this.blockLayer.visible = false;\r\n        this.floorLayer.hitArea = new Rectangle(0, 0, Laya.stage.width / this.MAPSCALE, Laya.stage.height / this.MAPSCALE);\r\n        this.floorLayer.on(Event.CLICK, this, this.onFloorLayerClick);\r\n        this.generatePassList();\r\n        this.addSelfToMap();\r\n        this.addAllPlayerToMap();\r\n        this.addAllEntityToMap();\r\n        this.initComplete();\r\n        this._map.setViewPortPivotByScale(0, 0);\r\n        this._map.scale = this.MAPSCALE;\r\n        this.onStageResize();\r\n        if (this.complete) {\r\n            GameUtils.runCallBack(this.complete);\r\n            this.complete = null;\r\n        }\r\n    }\r\n    private addAllPlayerToMap(): void {\r\n        var list: Array<PlayerVo> = ModelMap.ins.getPlayerList();\r\n        for (var i: number = 0; i < list.length; i++) {\r\n            var playerVo: PlayerVo = list[i];\r\n            this.addPlayer(playerVo, false);\r\n        }\r\n    }\r\n    private addAllEntityToMap(): void {\r\n        var list: Array<EntityVo> = ModelMap.ins.getEntityList();\r\n        for (var i: number = 0; i < list.length; i++) {\r\n            var entityVo: EntityVo = list[i];\r\n            this.addEntity(entityVo);\r\n        }\r\n    }\r\n    public tick(time: number): void {\r\n        if (!this._mainPlayer) return;\r\n        this._camera.tick();\r\n        for (var key in this.playerDic) {\r\n            this.playerDic[key].tick(time);\r\n        }\r\n        for (var key in this.entityDic) {\r\n            this.entityDic[key].tick(time);\r\n        }\r\n        for (var key in this.effectDic) {\r\n            this.effectDic[key].tick(time);\r\n        }\r\n        this.checkMask();\r\n        this.lockViewPort();\r\n    }\r\n\r\n    private checkMask(): void {\r\n        var spData: object;\r\n        for (var key in this.maskDataDic) {\r\n            var rect: Rectangle = new Rectangle(this.maskDataDic[key].x, this.maskDataDic[key].y, this.maskDataDic[key].width, this.maskDataDic[key].height);\r\n            if (rect.contains(this.mainPlayer.virtualX, this.mainPlayer.virtualY)) {\r\n                spData = this.maskDataDic[key];\r\n            }\r\n        }\r\n\r\n        if (spData) {\r\n            var startX = Math.max(0, spData[\"x\"] - this._camera.x);\r\n            var startY = Math.max(0, spData[\"y\"] - this._camera.y);\r\n            var endX = Math.min(Laya.stage.width / this.MAPSCALE, spData[\"x\"] + spData[\"width\"] - this._camera.x)\r\n            var endY = Math.min(Laya.stage.height / this.MAPSCALE, spData[\"y\"] + spData[\"height\"] - this._camera.y)\r\n            this.maskLayer.graphics.clear();\r\n            this.maskLayer.graphics.drawRect(0, 0, startX, Laya.stage.height / this.MAPSCALE, \"0x000000\");\r\n            this.maskLayer.graphics.drawRect(endX, 0, Laya.stage.width / this.MAPSCALE - endX, Laya.stage.height / this.MAPSCALE, \"0x000000\");\r\n            this.maskLayer.graphics.drawRect(startX, 0, endX - startX, startY, \"0x000000\");\r\n            this.maskLayer.graphics.drawRect(startX, endY, endX - startX, Laya.stage.height / this.MAPSCALE - endY, \"0x000000\");\r\n        }\r\n        else {\r\n            this.maskLayer.graphics.clear();\r\n        }\r\n    }\r\n\r\n    private lockViewPort(): void {\r\n        this._map.moveViewPort(this._camera.x, this._camera.y);\r\n    }\r\n    private addSelfToMap(): void {\r\n        var tile: Array<number> = this.randPassTile();\r\n        var data: object = {};\r\n        data[\"nickname\"] = ModelUser.ins.nickname;\r\n        data[\"role_id\"] = ModelUser.ins.role_id;\r\n        data[\"game_uid\"] = ModelUser.ins.game_uid;\r\n        data[\"x\"] = tile[0];\r\n        data[\"y\"] = tile[1];\r\n        data[\"direction\"] = RoleDir.Down;\r\n        data[\"color\"] = ModelUser.ins.color;\r\n        //手动把我加到自己的地图里\r\n        var playerVo: PlayerVo = ProtocolUtils.createPlayerVo(data);\r\n        this._mainPlayer = this.addPlayer(playerVo, false);\r\n        //广播给其他人把我加入地图\r\n        GameHandler.ins.send_addUser(data);\r\n    }\r\n    public isBlock(tileX: number, tileY: number): boolean {\r\n        var tileData: number = this.blockLayer.getTileData(tileX, tileY);\r\n        return tileData == BlockType.None || tileData == BlockType.Block;\r\n    }\r\n\r\n    public checkBlockInArea(rect: Rectangle): boolean {\r\n        var starttile: Point = this.getPosTile(rect.x, rect.y);\r\n        var endtile: Point = this.getPosTile(rect.x + rect.width, rect.y + rect.height);\r\n\r\n        for (var i: number = starttile.x; i <= endtile.x; i++) {\r\n            for (var j: number = starttile.y; j <= endtile.y; j++) {\r\n                if (this.isBlock(i, j)) {\r\n                    return false;\r\n                }\r\n            }\r\n        }\r\n\r\n        return true;\r\n    }\r\n\r\n    public checkCanPutEntity(mapPos: Point, config: EntityConfig): boolean {\r\n        var rect: Rectangle;\r\n        var x: number;\r\n        var y: number;\r\n        var w: number;\r\n        var h: number;\r\n        w = config[\"rect\"].width;\r\n        x = mapPos.x - w * 0.5;\r\n        if (config.isFloor) {\r\n            h = config[\"rect\"].height;\r\n            y = mapPos.y - h * 0.5;\r\n        }\r\n        else {\r\n            h = config.vGridNum * this._map.tileHeight;\r\n            y = mapPos.y - h;\r\n        }\r\n        rect = new Rectangle(x, y, w, h);\r\n        return this.checkBlockInArea(rect);\r\n    }\r\n\r\n    public generatePassList(): void {\r\n        this.passList.length = 0;\r\n        for (var col: number = 0; col < this._map.numColumnsTile; col++) {\r\n            for (var row: number = 0; row < this._map.numRowsTile; row++) {\r\n                if (!this.isBlock(col, row)) {\r\n                    this.passList.push([col, row]);\r\n                }\r\n            }\r\n        }\r\n    }\r\n    public randPassTile(): Array<number> {\r\n        return this.passList[RandomUtils.randInt(0, this.passList.length - 1)];\r\n    }\r\n    public getTilePos(tileX: number, tileY: number): Point {\r\n        var pos: Point = new Point();\r\n        pos.x = this._map.tileWidth * tileX + this._map.tileWidth / 2;\r\n        pos.y = this._map.tileHeight * tileY + this._map.tileHeight / 2;\r\n        return pos;\r\n    }\r\n    public getPosTile(x: number, y: number): Point {\r\n        var tile: Point = new Point();\r\n        tile.x = Math.floor(x / this.map.tileWidth);\r\n        tile.y = Math.floor(y / this.map.tileHeight);\r\n        return tile;\r\n    }\r\n    public get type(): SceneType {\r\n        return SceneType.Map;\r\n    }\r\n    public onStageResize(): void {\r\n        if (!this.mainPlayer) {\r\n            return;\r\n        }\r\n        this._map.changeViewPort(this._map.viewPortX, this._map.viewPortY, Laya.stage.width, Laya.stage.height);\r\n        this.floorLayer.hitArea = new Rectangle(0, 0, Laya.stage.width / this.MAPSCALE, Laya.stage.height / this.MAPSCALE);\r\n    }\r\n    public canPass(checkX: number, checkY: number): boolean {\r\n        return !this.isBlock(checkX, checkY);\r\n    }\r\n    private onFloorLayerClick(): void {\r\n        if (EntityManager.ins.status != EntityBuildStatus.None) {\r\n            return;\r\n        }\r\n        var tile: Point = this.getPosTile(this.floorLayer.mouseX + this.camera.x, this.floorLayer.mouseY + this.camera.y);\r\n        if (this.isBlock(tile.x, tile.y)) {\r\n            FloatMessage.showMessage(\"您点击的地方不能通行啊哈哈\");\r\n            return;\r\n        }\r\n        var myTile: Point = this.mainPlayer.getStandTile();\r\n        if (myTile.x == tile.x && myTile.y == tile.y) {\r\n            return;\r\n        }\r\n        //自己不必等广播消息回来 那样会有延迟\r\n        this.mainPlayer.startMove(tile.x, tile.y, RoleDir.None);\r\n        //关闭我自己的表演界面\r\n        TipUIManager.ins.closeTipById(TipUIKey.PerformTip);\r\n    }\r\n    public mapToScreenCoord(x: number, y: number): Point {\r\n        return new Point(x / this.MAPSCALE - this.camera.x, y / this.MAPSCALE - this.camera.y);\r\n    }\r\n    public screenToMapCoord(x: number, y: number): Point {\r\n        return new Point(x / this.MAPSCALE + this.camera.x, y / this.MAPSCALE + this.camera.y);\r\n    }\r\n    public getEntityByTile(x: number, y: number): EntityUnit {\r\n        for (var key in this.entityDic) {\r\n            var entityUnit: EntityUnit = this.entityDic[key];\r\n            var tile: Point = entityUnit.getStandTile();\r\n            if (x == tile.x && y == tile.y) {\r\n                return entityUnit;\r\n            }\r\n        }\r\n        return null;\r\n    }\r\n}","import GameScene from \"../scene/GameScene\";\r\nimport LayerManager from \"./LayerManager\";\r\nimport Sprite=Laya.Sprite;\r\nimport Handler=Laya.Handler;\r\nimport Tween=Laya.Tween;\r\nimport TimeUtils from \"../utils/TimeUtils\";\r\nimport Ease=Laya.Ease;\r\nimport GameUtils from \"../utils/GameUtils\";\r\nimport MapScene from \"../map/MapScene\";\r\nimport { GameZOrder } from '../constant/GameEnum';\r\nexport enum SceneType{\r\n    None,\r\n    Map\r\n}\r\nexport default class SceneManager{\r\n    private _curre
ackLayer.size(Laya.stage.width+1,Laya.stage.height);\r\n            this.blackLayer.mouseEnabled=true;\r\n           Laya.stage.addChild(this.blackLayer);\r\n            this.blackLayer.visible=false;\r\n        }\r\n        if(fadeIn)\r\n        {\r\n            this.blackLayer.visible=true;\r\n            this.blackLayer.alpha=0;\r\n            Tween.to(this.blackLayer,{alpha:1},300,Ease.linearNone,Handler.create(this,function(){\r\n                if(complete){\r\n                    GameUtils.runCallBack(complete);\r\n                }\r\n            }))\r\n        }\r\n        else\r\n        {\r\n            Tween.to(this.blackLayer,{alpha:0},300,Ease.linearNone,Handler.create(this,function(){\r\n                this.blackLayer.visible=false;\r\n                if(complete){\r\n                    GameUtils.runCallBack(complete);\r\n                }\r\n            }))\r\n        }\r\n    }\r\n    private static _ins: SceneManager = null;\r\n\tpublic static get ins(): SceneManager {\r\n\t\treturn SceneManager._ins = SceneManager._ins || new SceneManager();\r\n\t}\r\n}","import TipUIBase from \"../core/TipUIBase\";\r\nimport { editor } from \"../../editor/layaMaxUI\";\r\nimport { FaceConfig } from \"../../configs/face/FaceTable\";\r\nimport Handler=Laya.Handler;\r\nimport FaceUtils from \"../../model/face/FaceUtils\";\r\nimport Event=Laya.Event;\r\nimport GameHandler from \"../../protocol/handler/GameHandler\";\r\nimport { BehaviorType, InteractType } from \"../../constant/GameEnum\";\r\nimport Tween=Laya.Tween;\r\nimport Ease=Laya.Ease;\r\nimport SendTip from \"../core/SendTip\";\r\nimport InteractUtils from \"../../model/interact/InteractUtils\";\r\nimport { InteractConfig } from \"../../configs/interact/InteractTable\";\r\nimport PlayerVo from \"../../model/map/PlayerVo\";\r\nimport GameManager from '../../managers/GameManager';\r\nimport KeyEvent from '../../events/KeyEvent';\r\nimport GameUtils from '../../utils/GameUtils';\r\nimport InteractTable from '..
ocal.ts","../../src/GameMain.ts"],"sourcesContent":["\timport Loader=Laya.Loader;\r\n\texport default class ResPredef {\r\n\tpublic static getInitResList(): Array<any> {\r\n\t\tvar arr:Array<any> = [\r\n\t\t\t{ url: \"ui/common.atlas\", type: Loader.ATLAS, size: 100 },\r\n\t\t\t{ url: \"texture/share/caidai.atlas\", type: Loader.ATLAS, size: 100 }\r\n\t\t];\r\n\r\n\t\tfor (var i:number=1; i<=10; i++)\r\n\t\t{\r\n\t\t\tvar atlas:object = {};\r\n\t\t\tatlas[\"url\"] = \"texture/role_model_\" + i + \".atlas\";\r\n\t\t\tatlas[\"type\"] = Loader.ATLAS;\r\n\t\t\tatlas[\"size\"] = 100;\r\n\t\t\tarr.push(atlas);\r\n\r\n\t\t\tvar ani:object = {};\r\n\t\t\tani[\"url\"] = \"animations/role_model_\" + i + \".ani\";\r\n\t\t\tani[\"type\"] = Loader.JSON;\r\n\t\t\tani[\"size\"] = 100;\r\n\t\t\tarr.push(ani);\r\n\t\t}\r\n\r\n\t\treturn arr;\r\n\t}\r\n}\r\n","\r\nimport GlobalVars from '../GlobalVars';\r\nimport ItemTable  from '../configs/item/ItemTable';\r\nimport DictionaryTable from '../configs/dictionary/DictionaryTable';\r\nimport Loader = Laya.Loader;\r\nimport ShieldedTable from '../configs/shielded/ShieldedTable';\r\nimport SystemTable from '../configs/system/SystemTable';\r\nimport ConfigBase from '../configs/core/ConfigBase';\r\nimport ConfigKey from '../constant/ConfigKey';\r\nexport default class ConfigManager {\r\n\tprivate static configDic:object={};\r\n\tpublic static init(): void {\r\n\t\tConfigManager.loadConfig();\r\n\t}\r\n\tpublic static loadConfig(): void {\r\n\t\tvar JSZip = window[\"JSZip\"];\r\n\t\tvar dataBuffer: any = Loader.getRes(\"client.data\");\r\n\t\tvar zip: any = new JSZip(dataBuffer);\r\n\t\tvar files: Array<any> = zip.files;\r\n\t\tfor (var key in files) {\r\n\t\t\tvar fileData: any = files[key];\r\n\t\t\tvar arr: string = fileData.name.split(\"/\");\r\n\t\t\tvar tableName: string = arr[arr.length - 1];\r\n\t\t\ttableName = tableName.split(\".\")[0];\r\n\t\t\tif (tableName) {\r\n\t\t\t\tvar zipFile: any = zip.file(fileData.name);\r\n\t\t\t\tif (z